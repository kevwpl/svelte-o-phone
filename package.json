{
  "name": "@kevwpl/svelte-o-phone",
  "version": "0.0.9",
  "type": "module",
  "private": false,
  "license": "MIT",
  "sideEffects": false,
  "svelte": "./src/lib/index.js",
  "exports": {
    ".": {
      "svelte": "./src/lib/index.js",
      "import": "./src/lib/index.js",
      "require": "./src/lib/index.js",
      "default": "./src/lib/index.js"
    },
    "./usePhonePicker": {
      "svelte": "./src/lib/svelte-o-phone/usePhonePicker.svelte.js",
      "import": "./src/lib/svelte-o-phone/usePhonePicker.svelte.js",
      "require": "./src/lib/svelte-o-phone/usePhonePicker.svelte.js",
      "default": "./src/lib/svelte-o-phone/usePhonePicker.svelte.js"
    },
    "./countries": {
      "svelte": "./src/lib/svelte-o-phone/countries.js",
      "import": "./src/lib/svelte-o-phone/countries.js",
      "require": "./src/lib/svelte-o-phone/countries.js",
      "default": "./src/lib/svelte-o-phone/countries.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src/lib/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@internationalized/date": "^3.10.0",
    "@lucide/svelte": "^0.544.0",
    "@shikijs/langs": "^3.13.0",
    "@shikijs/themes": "^3.13.0",
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/kit": "^2.43.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@tailwindcss/vite": "^4.1.13",
    "bits-ui": "^2.11.4",
    "clsx": "^2.1.1",
    "isomorphic-dompurify": "^2.28.0",
    "package-manager-detector": "^1.4.0",
    "runed": "^0.31.1",
    "shiki": "^3.13.0",
    "svelte": "^5.39.5",
    "svelte-check": "^4.3.2",
    "svelte-toolbelt": "^0.9.3",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.7"
  },
  "dependencies": {
    "libphonenumber-js": "^1.12.23"
  },
  "peerDependencies": {
    "svelte": ">=5.0.0"
  },
  "description": "A flexible, headless phone number input component for Svelte 5 (with runes), powered by `libphonenumber-js`.",
  "main": "svelte.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevwpl/svelte-o-phone.git"
  },
  "keywords": [
    "svelte",
    "phone",
    "input",
    "telephone",
    "tel",
    "headless"
  ],
  "author": "kevwpl",
  "bugs": {
    "url": "https://github.com/kevwpl/svelte-o-phone/issues"
  },
  "homepage": "https://github.com/kevwpl/svelte-o-phone#readme"
}
